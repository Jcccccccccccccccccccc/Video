
 uni-video {
	margin-top: 20rpx;
	margin-left: 5%;
}
.add_picture {
	width: 100%;
	height: 500rpx;
	background-color: #f8faf7;
	margin-top: 5rpx;
}
.many_photo {
	border: #8a8a8a 0.1rpx dashed;
	width: 300rpx;
	height: 300rpx;
	border-radius: 8rpx;
	margin: 30rpx;
}
.many_photo>image {
	width: 100rpx;
	height: 100rpx;
	margin: 30% 30%;
}
.video-box {
	width: 400rpx;
	height: 300rpx;
	border: #8a8a8a 0.1rpx solid;
	border-radius: 8rpx;
	margin: 45rpx 100rpx;
}
.video-box>video {
	width: 400rpx;
	height: 250rpx;
}
.video-box>image {
	display: block;
	height: 36rpx;
	width: 36rpx;
	margin-left: 351rpx;
	margin-bottom: 10rpx;
}
.sub-button {
	width: 500rpx;
	height: 70rpx;
	margin: 84rpx 120rpx;
	background-color: #66b94d;
	color: #f5fef2;
	line-height: 70rpx;
}

