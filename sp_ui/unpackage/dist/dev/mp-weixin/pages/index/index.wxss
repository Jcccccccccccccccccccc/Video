
.content {
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	height: 950rpx;
}
.logo {
	height: 200rpx;
	width: 200rpx;
	margin-top: 200rpx;
	margin-left: auto;
	margin-right: auto;
	margin-bottom: 50rpx;
}
.text-area {
	display: flex;
	justify-content: center;
}
.title {
	font-size: 36rpx;
	color: #8f8f94;
}
.video-box {
	width: 100%;
	height: 950rpx;
}
.button-box {
	width: 100%;
	height: 150rpx;
	margin-top: 6rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.button-box image {
	width: 15%;
	height: 125rpx;
	margin: 0 15px;
}

